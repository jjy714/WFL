{
  "expo": {
    "name": "WFL",
    "slug": "wfl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/8382732.png",
    "scheme": "WFL_APP", 
    "userInterfaceStyle": "light",
    "splash": {
      "image": "/Users/jason/Projects/HCIAppProject/WFL/assets/bundle/iOS/LaunchImage.launchimage",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ssabsari.wfl"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ssabsari.wfl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SFPRODISPLAYREGULAR.otf"
          ]
        }
      ],
      [
        "@mj-studio/react-native-naver-map",
        {
          "client_id": "{{Naver Map Client Key}}",
          "android": {
            "ACCESS_FINE_LOCATION": true,
            "ACCESS_COARSE_LOCATION": true,
            "ACCESS_BACKGROUND_LOCATION": true
          },
          "ios": {
            "NSLocationAlwaysAndWhenInUseUsageDescription": "{{ your location usage description }}",
            "NSLocationWhenInUseUsageDescription": "{{ your location usage description }}",
            "NSLocationTemporaryUsageDescriptionDictionary": {
              "purposeKey": "{{ your purpose key }}",
              "usageDescription": "{{ your location usage description }}"
            }
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8b8b04cb-0233-4152-af9e-c63c85665fa5"
      }
    },
    "owner": "ssabsari"
  }
}
